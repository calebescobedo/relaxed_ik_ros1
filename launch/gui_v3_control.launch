<?xml version="1.0"?>
<launch>
    <arg name="flag" default="xbox" />
    <arg name="launch_rviz" default="true"/>
    <include file="$(find franka_example_controllers)/launch/hiro_joint_impedance_example_controller.launch" pass_all_args="true"/>
    <include file="$(find relaxed_ik_ros1)/launch/demo.launch" pass_all_args="true"/>
    <node pkg="relaxed_ik_ros1" type="robo_dialysis_v3.py" name="xbox_input" output="screen">
        <param name="flag" value="$(arg flag)" />
    </node>
    <node pkg="joy" type="joy_node" name="joy" output="screen">
        <param name="autorepeat_rate" value="100"/>
    </node>
</launch>